# Copy this file to .env or .env.local and fill in your credentials.
# All values are read by example helpers via dotenv.

# Binance REST key pair (required for trade adapter examples)
BINANCE_API_KEY=your-binance-key
BINANCE_API_SECRET=your-binance-secret

# OKX REST key pair (required) + passphrase
OKX_API_KEY=your-okx-key
OKX_API_SECRET=your-okx-secret
OKX_PASSPHRASE=your-okx-passphrase

# Toggle simulated mode (true skips real order placement)
SIMULATED=true

# Optional: global HTTP proxy for adapters (e.g. http://127.0.0.1:7890)
PROXY=

# Optional: request timeout in milliseconds
TIMEOUT=15000
